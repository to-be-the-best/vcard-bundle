{
    "name": "to-be-the-best/vcard-bundle",
    "type": "library",
    "description": "This Symfony 2 bundle can generate vCards with lots of data. When using an < iOS 7 device it will export as a .ics file because iOS devices don't support the default .vcf files.",
    "keywords": ["symfony2", "bundle", "vcard", "generator", ".vcf", "php"],
    "homepage": "https://github.com/jeroendesloovere/vcard-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Jeroen Desloovere",
            "email": "info@jeroendesloovere.be",
            "homepage": "http://jeroendesloovere.be",
            "role": "Developer"
        }
    ],
    "repositories": [
        {
            "url": "https://github.com/to-be-the-best/vcard.git",
            "type": "git"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "to-be-the-best/vcard": "~4.0"
    },
    "require-dev": {
        "symfony/framework-bundle": "~3.0"
    },
    "autoload": {
        "psr-4": { "JeroenDesloovere\\Bundle\\VCardBundle\\": "" }
    }
}
